        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Trang kiểm định - Nhân viên" Height="350" Width="600">
    <Grid Background="#f0f0f0">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Text="Chào mừng nhân viên kiểm định!" 
                       FontSize="22" FontWeight="Bold"
                       HorizontalAlignment="Center" Margin="0,0,0,20"/>
            <Button Content="Ghi kết quả kiểm định" Width="200" Height="40" Margin="0,10"/>
            <Button Content="Xem danh sách xe cần kiểm" Width="200" Height="40" Margin="0,10"/>
        </StackPanel>
    </Grid>
</Window>
