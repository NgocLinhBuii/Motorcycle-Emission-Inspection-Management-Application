<Window x:Class="Motorcycle_Emission_Inspection_Management.Dashboards.InspectorDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Motorcycle_Emission_Inspection_Management.Dashboards"
        mc:Ignorable="d"
        Title="InspectorDashboard" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Xin chào, Công nhân kiểm định" FontSize="24" FontWeight="Bold" Margin="0 0 0 20" />
            <Button Content="Thực hiện kiểm định" Margin="0 0 0 10"/>
            <Button Content="Xác nhận kết quả" Margin="0 0 0 10"/>
            <Button Content="Đăng xuất" Margin="0 20 0 0"/>
        </StackPanel>
    </Grid>
</Window>
